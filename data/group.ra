LOAD EMPLOYEEGROUP1
G1 <- GROUP BY DEPARTMENT_ID FROM EMPLOYEEGROUP1 HAVING AVG(SALARY) < 1000 RETURN AVG(SALARY)
G2 <- GROUP BY DEPARTMENT_ID FROM EMPLOYEEGROUP1 HAVING MAX(SALARY) < 7 RETURN MIN(SALARY)
G3 <- GROUP BY DEPARTMENT_ID FROM EMPLOYEEGROUP1 HAVING MIN(SALARY) < 7 RETURN MAX(SALARY)
G4 <- GROUP BY DEPARTMENT_ID FROM EMPLOYEEGROUP1 HAVING COUNT(SALARY) < 7 RETURN AVG(SALARY)
G5 <- GROUP BY DEPARTMENT_ID FROM EMPLOYEEGROUP1 HAVING SUM(SALARY) >= 7 RETURN AVG(SALARY)
